<Worksheet ss:Name="Рейтинг">
  <Table>
    <Column ss:Width="100"/>
    <Column ss:Width="100"/>
    <Column ss:Width="100"/>
    <Column ss:Width="100"/>
    <Column ss:Width="100"/>
    <Column ss:Width="100"/>
    <Column ss:Width="100"/>

    <Row>
      <Cell ss:StyleID="sGreen"><Data ss:Type="String">ФИО</Data></Cell>
      <Cell ss:StyleID="sGreen"><Data ss:Type="String">Эл. почта</Data></Cell>
      <Cell ss:StyleID="sGreen"><Data ss:Type="String">Группа</Data></Cell>
      <Cell ss:StyleID="sGreen"><Data ss:Type="String">Курс</Data></Cell>
      <Cell ss:StyleID="sGreen"><Data ss:Type="String">Дата окончания обучения</Data></Cell>
      <Cell ss:StyleID="sGreen"><Data ss:Type="String">Место в рейтинге</Data></Cell>
      <Cell ss:StyleID="sGreen"><Data ss:Type="String">Баллы</Data></Cell>
    </Row>

    <% @group_subscriptions.each do |gs| %>
        <Row>
          <% student = gs.student || Student.new %>
          <Cell><Data ss:Type="String"><%= student.full_name %></Data></Cell>
          <Cell><Data ss:Type="String"><%= student.email %></Data></Cell>
          <Cell><Data ss:Type="String"><%= gs.group_title %></Data></Cell>
          <Cell><Data ss:Type="String"><%= gs.course.name %></Data></Cell>
          <Cell><Data ss:Type="String"><%= format_date(gs.end_on) %></Data></Cell>
          <Cell><Data ss:Type="String"><%= gs.rating_place %></Data></Cell>
          <Cell><Data ss:Type="String"><%= gs.rating_score %></Data></Cell>
        </Row>
    <% end %>
  </Table>
</Worksheet>