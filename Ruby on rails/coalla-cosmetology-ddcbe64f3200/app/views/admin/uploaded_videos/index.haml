%main#main.main.ctn
  .grid.grid__collapse
    .grid_i.grid_i__grid-2-9.sidebar>
      = render 'common/admin_sidebar'
    .grid_i.grid_i__grid-7-9>
      = render 'admin/breadcrumbs', crumbs: {'видео' => admin_uploaded_videos_path}
      %h3.tx-center ВИДЕО
      .form__admin
        .admin_header.tx-center
          .header_inner
            = link_to new_admin_uploaded_video_path, class: 'a_under' do
              %span.icon-add> +
              Добавить видео
        .admin_body.table.table_base-color
          .table_head
            .grid
              .grid_i.grid_i__grid-7-9>
                %p название
              .grid_i.grid_i__grid-2-9>
                %p.tx-right
                  действия
          .grid.grid__collapse
            = render collection: @uploaded_videos, partial: 'uploaded_video'
      = will_paginate @uploaded_videos, renderer: PageLinkRenderer, inner_window: 1, outer_window: 0