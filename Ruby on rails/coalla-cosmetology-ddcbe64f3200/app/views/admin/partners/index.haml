%main#main.main.ctn
  .grid.grid__collapse
    .grid_i.grid_i__grid-2-9.sidebar>
      = render 'common/admin_sidebar'
    .grid_i.grid_i__grid-7-9>
      = render 'admin/breadcrumbs', crumbs: {'партнеры' => admin_partners_path}
      %h3.tx-center Партнеры
      .form__admin
        .admin_header.tx-center
          .header_inner
            = link_to new_admin_partner_path, class: 'a_under' do
              %span.icon-add> +
              Добавить партнера
        .admin_body.table.table_base-color
          .table_head
            .grid
              .grid_i.grid_i__grid-8-9>
                %p название
              .grid_i.grid_i__grid-1-9>
                %p.tx-center
                  действия
          .grid.grid__collapse
            = render @partners
      = will_paginate @partners, renderer: PageLinkRenderer, inner_window: 1, outer_window: 0